{
  "name": "@efx/layout-react",
  "version": "0.1.0",
  "description": "React components for EfxLayout - CSS Grid-based dashboard layouts",
  "type": "module",
  "license": "MIT",
  "author": "",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/efx-layout-react"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@efx/layout-core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.0",
    "react": "^19.2.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.2"
  }
}
